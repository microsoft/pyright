{
    "name": "pyright-internal",
    "displayName": "pyright",
    "description": "Type checker for the Python language",
    "version": "1.1.334",
    "license": "MIT",
    "private": true,
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "tsc",
        "clean": "shx rm -rf ./dist ./out",
        "test": "jest --forceExit",
        "test:coverage": "jest --forceExit --reporters=jest-junit --reporters=default --coverage --coverageReporters=cobertura --coverageReporters=html --coverageReporters=json",
        "test:imports": "jest importResolver.test --forceExit --runInBand"
    },
    "dependencies": {
        "@iarna/toml": "2.2.5",
        "@yarnpkg/fslib": "2.10.3",
        "@yarnpkg/libzip": "2.3.0",
        "chalk": "^4.1.2",
        "chokidar": "^3.5.3",
        "command-line-args": "^5.2.1",
        "jsonc-parser": "^3.2.0",
        "leven": "^3.1.0",
        "source-map-support": "^0.5.21",
        "tmp": "^0.2.1",
        "vscode-jsonrpc": "8.1.0",
        "vscode-languageserver": "8.1.0",
        "vscode-languageserver-textdocument": "1.0.10",
        "vscode-languageserver-types": "3.17.3",
        "vscode-uri": "^3.0.7"
    },
    "devDependencies": {
        "@types/command-line-args": "^5.2.0",
        "@types/fs-extra": "^11.0.1",
        "@types/jest": "^29.5.3",
        "@types/node": "^17.0.45",
        "@types/tmp": "^0.2.3",
        "jest": "^29.6.1",
        "jest-junit": "^16.0.0",
        "shx": "^0.3.4",
        "ts-jest": "^29.1.1",
        "typescript": "~4.4.4",
        "word-wrap": "1.2.4"
    }
}
