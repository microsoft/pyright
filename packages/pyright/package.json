        "ts-loader": "^8.0.11",
        "tsconfig-paths-webpack-plugin": "^3.3.0",
        "typescript": "^4.0.5",
        "webpack": "^5.4.0",
        "webpack-cli": "^4.2.0"
    },
    "files": [
        "/dist"
    ],
    "main": "index.js",
    "bin": {
        "pyright": "index.js",
        "pyright-langserver": "langserver.index.js"
    }
