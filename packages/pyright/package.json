        "webpack": "cross-env NODE_ENV=\"development\" webpack --progress"
    },
    "devDependencies": {
        "@types/copy-webpack-plugin": "^6.3.0",
        "@types/node": "^12.19.9",
        "@types/webpack": "^4.41.25",
        "clean-webpack-plugin": "^3.0.0",
        "copy-webpack-plugin": "^7.0.0",
        "cross-env": "^7.0.3",
        "shx": "^0.3.3",
        "ts-loader": "^8.0.12",
        "ts-node": "^9.1.1",
        "tsconfig-paths-webpack-plugin": "^3.3.0",
        "typescript": "^4.1.3",
        "webpack": "^5.10.1",
        "webpack-cli": "^4.2.0"
    },
    "files": [
        "/dist"
    ],
    "main": "index.js",
    "bin": {
        "pyright": "index.js",
        "pyright-langserver": "langserver.index.js"
    }
