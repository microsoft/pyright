{
  "name": "pyright-lsif",
  "version": "0.1.0",
  "description": "LSIF indexer for Python",
  "main": "index.js",
  "scripts": {
    "build": "webpack --mode production --progress",
    "clean": "shx rm -rf ./dist ./out README.md LICENSE.txt",
    "prepack": "npm run clean && shx cp ../../README.md . && shx cp ../../LICENSE.txt . && npm run build",
    "webpack": "webpack --mode development --progress",
    "watch": "webpack --mode development --progress --watch",
    "test": "jest --forceExit --watchAll --silent=false"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@sourcegraph/eslint-config": "0.26.0",
    "@sourcegraph/prettierrc": "3.0.3",
    "@sourcegraph/tsconfig": "4.0.1",
    "@types/diff": "^5.0.2",
    "@types/glob": "^7.2.0",
    "@types/google-protobuf": "^3.15.5",
    "@types/jest": "^27.4.0",
    "copy-webpack-plugin": "^10.2.0",
    "jest-junit": "^13.0.0",
    "shx": "^0.3.3",
    "ts-jest": "^27.1.3",
    "ts-loader": "^9.2.6",
    "tsconfig-paths-webpack-plugin": "^3.5.2",
    "typescript": "^4.5.4",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1"
  },
  "dependencies": {
    "diff": "^5.0.0",
    "glob": "^7.2.0",
    "google-protobuf": "^3.19.3",
    "jest": "^27.5.0",
    "ts-node": "^10.5.0",
    "uvu": "^0.5.3",
    "vscode-languageserver": "^7.0.0"
  },
  "files": [
    "/dist",
    "LICENSE.txt"
  ],
  "bin": {
    "lsif-pyright": "index.js"
  }
}
